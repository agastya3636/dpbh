{
    "manifest_version": 3,
    "name": "dpbh",
    "description": "This extension displays data from the current page",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      
    },
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    }
  }
  